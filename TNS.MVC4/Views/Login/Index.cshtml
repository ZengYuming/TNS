@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<select id="txtUserName" class="easyui-combobox" name="userName" style="width: 200px;">
    @{ 
        HttpCookie cookie = Response.Cookies.Get(TNS.Metadata.Const.LoginAccountList_CookieName);
        if (cookie != null && cookie.Value != null)
        {
            foreach (string userName in cookie.Value.Split(','))
            { 
        <option value="@userName">@userName</option>          
            }
        }
    }
</select>

<input type="button" title="login" onclick="login()" />
<script type="text/javascript">
    function login() {
        $.ajax({
            type: "POST",
            async: false,
            url: '/Login/Login',
            success: function (re) {
                alert('Ok');
            }
        });
    }
</script>
